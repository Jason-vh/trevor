# SquashCity credentials
SQUASH_CITY_USERNAME=your_username
SQUASH_CITY_PASSWORD=your_password

# Telegram + webhook
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=123456789,-987654321   # comma separated allowlist
TELEGRAM_WEBHOOK_SECRET=super-secret-segment

# OpenAI / AI SDK
OPENAI_API_KEY=sk-...
OPENAI_MODEL=gpt-4.1-mini

# Server + scheduler
PORT=3000
MONITOR_CRON=*/15 * * * *
MONITOR_LOOKAHEAD_DAYS=7
MONITOR_TIMEZONE=Europe/Amsterdam

# Axiom logging (optional)
AXIOM_TOKEN=your_axiom_token_here
AXIOM_DATASET=your_dataset_name_here
